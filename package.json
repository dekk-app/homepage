  "scripts": {
    "prebuild": "yarn mock:sw:clean",
    "build": "next build",
    "cypress:run": "npx cypress run --record",
    "cypress:run-local": "npx cypress run",
    "cypress:open": "npx cypress open",
    "dev": "next",
    "graphql:setup": "node prepare/graphql-setup.js",
    "graphql:types": "graphql-codegen",
    "lint": "yarn xo && yarn stylelint",
    "mock:sw": "npx cpy-cli prepare/mockServiceWorker.js public",
    "mock:sw:clean": "npx rimraf 'public/mockServiceWorker.js'",
    "ncu": "npx npm-check-updates -u --dep='prod,dev'",
    "ncu:minor": "yarn ncu -t minor",
    "ncu:patch": "yarn ncu -t patch",
    "prepare": "husky install; yarn graphql:types",
    "prettier": "prettier 'src/**/*.{ts,tsx}'",
    "prettier:write": "yarn prettier --write",
    "release": "standard-version",
    "spj": "npx sort-package-json package.json",
    "start": "next start",
    "stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "pretest": "yarn mock:sw",
    "test": "npx start-server-and-test start http://localhost:3000 cypress:run",
    "posttest": "yarn mock:sw:clean",
    "pretest:local": "yarn mock:sw",
    "test:local": "npx start-server-and-test start http://localhost:3000 cypress:run-local",
    "posttest:local": "yarn mock:sw:clean",
    "unimported": "npx unimported",
    "xo": "xo",
    "xo:fix": "yarn xo --fix"
  },
